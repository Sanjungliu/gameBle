<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

    <%- include('../partials/navbar2', {PlayerId}) %>

    <h2>
        Players List
    </h2>

    <table border="1">
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
            <th>Favourite</th>
            <th>Option</th>
        </tr>
        <% data.forEach(el => { %>
        <tr>
            <td><%= el.id %> </td>
            <td><%= el.username %></td>
            <td><%= el.email %></td>
            <td><%= el.favourite_genre %> </td>
            <td><a href="/players/<%= el.id %>">See</a>
            </td>
        </tr>
        <% }) %>

    </table>
</body>
</html>